import java.util.*;
class  Main {
    public static void main(String[] args) {
        int[] arr = {45, 63, 25, 12, 7, -21, -1, 0, -7, 55};
        System.out.println(min(arr));

    }

    static int min(int[] arr) {
        int ans = arr[0];
        for (int i = 1; i < arr.length; i++) {
            if (arr[i] < ans) {
                ans = arr[i];

            }
        }
        return ans; //don't give the return statement inside the if condition

    }
}

