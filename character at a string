import java.util.*;
class Main
{
    public static void main(String []args )
    {
        String name="sanjay";
        char target='y';
        System.out.print(find(name,target));
    }
    static boolean find(String name,char target)
    {
        if(name.length()==0)
        {
            return false;
        }
        for(int i=0;i<name.length();i++)
        {
            if(name.charAt(i)==target)
            {
                return true;
            }
        }
        return false;
    }
}
