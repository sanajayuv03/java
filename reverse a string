import java.util.*;
class Main
{
    public static void main(String [] args)
    {
        String sentence="sanjay likes zoho";
        System.out.print(revers(sentence));
    }
    static String revers(String sentence)
    {
        String [] arr=sentence.split("\\s+");
        StringBuilder revers1=new StringBuilder();
        for(int i=arr.length-1;i>=0;i--)
        {
            revers1.append(arr[i]).append(" ");
        }
        return revers1.toString();
    }
}
