import java.util.*;
class main
{
    public static void main(String [] args)
    {
        print (1);
    }
    static void print(int n)
    {
        if(n==5)
        {
            System.out.println(5);
            return;
        }
        System.out.println(n);
        print(n+1);
        //recursive call
        //if you are calling a function again and again ;you can treat it as a separate stack
    }
}
